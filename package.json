{
  "name": "fornoveo",
  "version": "1.0.0",
  "description": "Test project for Noveo",
  "main": "util/logger.js",
  "scripts": {
    "build": "NODE_ENV=production babel",
    "prepublishOnly": "npm run build",
    "selenium-install": "selenium-standalone install",
    "pretest": "rm -rf allure-results",
    "test": "rm -rf allure-results && multi='spec=- mocha-allure-reporter=-' mocha ./test/start.js",
    "selenium": "selenium-standalone start",
    "report": "allure generate allure-results --clean -o allure-report",
    "report-open": "allure open allure-report"
  },
  "author": "Evgeniy Zvonkovich",
  "license": "ISC",
  "dependencies": {
    "allure-commandline": "^2.13.0",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "mocha-allure-reporter": "^1.4.0",
    "mocha-multi": "^1.1.3",
    "npm-run-all": "^4.1.5",
    "selenium-standalone": "^6.17.0",
    "selenium-webdriver": "^4.0.0-alpha.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7"
  }
}
